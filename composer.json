{
    "name": "adigitalco/enhanced-belongs-to-many",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Adigitalco\\EnhancedBelongsToMany\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adigitalco\\EnhancedBelongsToMany\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
